<h1><i>Current selection</i></h1>
 <div class="container">
  <div class="row">
      <% @cars.each do |car| %>
        <div class="col-sm-4">
          <div class="card-car">
            <img src="https://tesla-cdn.thron.com/delivery/public/image/tesla/c82315a6-ac99-464a-a753-c26bc0fb647d/bvlatuR/std/1200x628/lhd-model-3-social">
            <div class="card-car-infos">
              <p><b>   <%= car.make  %></b></p>
              <p><b>   <%= car.model  %></b></p>
              <p><b>   <%= car.year  %></b></p>
              <ul class="nav-links">
              <%= link_to 'View', car_path(car) %>
              </ul>
            </div>
          </div>
        </div>
       <% end %>
      </div>
    </div>

<h1>Vroom</h1>
<p>all the cars!</p>
<div class="row">
    <% @cars.each do |car| %>
      <div class="col-sm-4">
        <div class="card-car">
          <img src="https://tesla-cdn.thron.com/delivery/public/image/tesla/c82315a6-ac99-464a-a753-c26bc0fb647d/bvlatuR/std/1200x628/lhd-model-3-social">
          <div class="card-car-infos">
            <p><b>   <%= car.name  %></b></p>
            <ul class="nav-links">
            <li><a href="/cars/`${car_id}`">Edit</a></li>
            </ul>
          </div>
        </div>
      </div>
     <% end %>

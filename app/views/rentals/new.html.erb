<br>
<h3><center>Book a <%= @rental.car.make %>&nbsp;<%= @rental.car.model %></center></h3>


<div class="container">
  <%= simple_form_for([@cars, @rental]) do |f| %>
    <%= f.input :date_rented %>
    <%= f.input :date_return %>
    <%= f.input :owner_approval, :as => :hidden %>
    <%= f.submit value="Confirm" %>
  <% end %>
</div>

